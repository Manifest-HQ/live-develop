{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev:server": "nuxt dev --host '0.0.0.0' --port 9001",
    "dev": "nuxt dev --port 9001",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "ios:live": "node switch-live-app-dev.js enable && bun run generate && npx cap sync && npx cap open ios && bun run dev:server",
    "ios:open": "node switch-live-app-dev.js disable && bun run generate && npx cap sync && npx cap open ios",
    "android:live": "node switch-live-app-dev.js enable && bun run generate && npx cap sync && npx cap open android && bun run dev:server",
    "android:open": "node switch-live-app-dev.js disable && bun run generate && npx cap sync && npx cap open android"
  },

{
  "buildCommand": "cd app && bun i && bun run generate",
  "outputDirectory": "app/.output/public",
  "installCommand": "cd app && bun install",
  "framework": "nuxtjs",
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/routes/:path*" }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}

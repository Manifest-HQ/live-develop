{
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ],
  "buildCommand": "cd app && bun run generate",
  "outputDirectory": "app/.output/public",
  "installCommand": "cd app && bun install",
  "framework": "nuxtjs",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
